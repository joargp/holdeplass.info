@model IEnumerable<GtfsService.Models.Stop>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table>
    <tr>
        <th></th>
        <th>
            Lat
        </th>
        <th>
            ZoneId
        </th>
        <th>
            Lon
        </th>
        <th>
            Url
        </th>
        <th>
            Description
        </th>
        <th>
            Name
        </th>
        <th>
            LocationType
        </th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.StopId }) |
            @Html.ActionLink("Details", "Details", new { id=item.StopId }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.StopId })
        </td>
        <td>
			@item.Lat
        </td>
        <td>
			@item.ZoneId
        </td>
        <td>
			@item.Lon
        </td>
        <td>
			@item.Url
        </td>
        <td>
			@item.Description
        </td>
        <td>
			@item.Name
        </td>
        <td>
			@item.LocationType
        </td>
    </tr>
}

</table>


