@model IEnumerable<GtfsService.Models.Trip>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table>
    <tr>
        <th></th>
        <th>
            TripNumber
        </th>
        <th>
            Headsign
        </th>
        <th>
            ShapeId
        </th>
        <th>
            ServiceId
        </th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
        <td>
			@item.TripNumber
        </td>
        <td>
			@item.Headsign
        </td>
        <td>
			@item.ShapeId
        </td>
        <td>
			@item.ServiceId
        </td>
    </tr>
}

</table>


